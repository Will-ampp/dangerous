    "@types/react-dom": "16.0.11",
    "@types/react-is": "16.5.0",
    "babel-loader": "^8.0.5",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "rollup": "^1.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
